{
	"name": "tjm/sy-console"
	,"description": "Symfony Console component with dependency injection, config handling, configurable command loading, and other niceties."
	,"authors": [
		{
			"name": "Toby Mackenzie"
			,"email": "public@tobymackenzie.com"
			,"homepage": "http://www.tobymackenzie.com"
		}
	]
	,"license": "0BSD"
	,"prefer-stable": true
	,"require" : {
		"php": ">=8"
		,"symfony/config": ">=4.4 <7.0"
		,"symfony/console": ">=4.4 <7.0"
		,"symfony/dependency-injection": ">=4.4 <7.0"
		,"symfony/event-dispatcher":  ">=4.4 <7.0"
		,"symfony/finder": ">=4.4 <7.0"
		,"tjm/sy-dependency-injection": "^2.0"
	}
	,"require-dev": {
		"symfony/yaml": ">=4.4 <7.0",
		"tjm/dev": "^1"
	}
	,"autoload" : {
		"psr-4" : {
			"TJM\\Component\\Console\\" : "src"
		}
	}
	,"autoload-dev": {
		"psr-4": {
			"TJM\\Console\\Tests\\": "tests"
		}
	}
}
